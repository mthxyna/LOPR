Algoritmo "Questao 7"
Var
  valorReal, valorDolar: real
  resultado: real
Inicio
    escreval ("Informe o valor do dólar hoje ")
    leia (valorDolar)
    escreval ("Informe o valor do real ")
    leia (valorReal)
    resultado<- valorDolar*valorReal
    escreval ("Equivalem a ", resultado)

Fimalgoritmo